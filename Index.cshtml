<h1>Html.DisplayFor Example</h1>
@model WebApplication30.Models.DisplayforModel;
@using (Html.BeginForm("Index", "Displayfor", FormMethod.Post))
{
    <span>Enter Your Name:</span>@Html.TextBoxFor(m => m.UserName)
    <br />
    <br />
    <input id = "Submit" type = "submit" value = "submit" />
       
}
 
<hr />
 
<strong>Strongly Typed Value:</strong> @Html.DisplayFor(m => m.UserName)<br />
 
<strong>Loosely &nbsp;Typed Value: </strong> @Html.Display("UserName")

